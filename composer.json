{
    "name": "railken/amethyst-cycle",
    "type": "library",
    "description": "Define a cycle for a subscription, a contract, an activity. Keep track of each cycle defining dates and statuses",
    "keywords": ["laravel", "amethyst", "amethyst-cycle", "cycle"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.*",
        "railken/amethyst-foo": "0.1.*"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "cycle",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\CycleServiceProvider"
            ]
        }
    }
}
